{% macro dataFormat(cell, spreadSheet) %}
    {% set styleName = cell.getStyleName() %}
    {% set cellStyle = spreadSheet.getStyle(styleName) %}
    {% set dataStyleName = cellStyle.getDataStyleName() %}
    {% set dataFormat = spreadSheet.getDataStyle(dataStyleName).toNumeralStringFormat() %}
    {% if dataFormat != '' %}
        data-format="{{ dataFormat }}"
    {% endif %}
{% endmacro %}
