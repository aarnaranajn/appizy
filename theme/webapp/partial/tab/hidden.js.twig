// Hidden tab '{{ tab.getName() }}'
window.APY = window.APY || {};
window.APY.cells ={};
{% spaceless %}
{%- for row in tab.getRows() %}
    {%- for cell in row.getCells() %}
        {%- if cell.getType() in ['in', 'out'] -%}
            {%- if cell.getValueType() in ['float', 'currency', 'number'] -%}
                APY.cells.{{ cell.getName() }} = {{ cell.getValue() }};
            {%- else -%}
                APY.cells.{{ cell.getName() }}= '{{ cell.getValue() }}';
            {%- endif -%}
        {%- endif -%}
    {%- endfor -%}
{%- endfor -%}
{% endspaceless %}
